<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="RoomSearch.CalendarFragment">


    <DatePicker
        android:layout_width="245dp"
        android:layout_height="390dp"
        android:id="@+id/datePicker2"
        android:scaleX="0.8"
        android:scaleY="0.8"
        android:layout_marginLeft="-30dp"
        android:layout_marginTop="-40dp" />

    <Spinner
        android:layout_height="wrap_content"
        android:layout_marginTop="81dp"
        android:id="@+id/spinner8"
        android:layout_below="@+id/autoCompleteTextView"
        android:spinnerMode="dialog"
        android:layout_alignStart="@+id/autoCompleteTextView"
        android:layout_width="150dp" />

    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="lol.wepekchek.istd.sutdbookingrooms.Map.MapFragment"
        android:layout_centerHorizontal="true"
        android:id="@+id/fragment2"
        android:layout_below="@+id/datePicker2"
        android:layout_marginTop="-30dp" />

    <AutoCompleteTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/autoCompleteTextView"
        android:layout_marginTop="11dp"
        android:dropDownWidth="match_parent"
        android:text="Choose"
        android:textSize="14sp"
        android:layout_below="@+id/textView5"
        android:layout_alignStart="@+id/textView5" />

    <TextView
        android:text="Select a room:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:textSize="23sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="12dp" />
</RelativeLayout>
